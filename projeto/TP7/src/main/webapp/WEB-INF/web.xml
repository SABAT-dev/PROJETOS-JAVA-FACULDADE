<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>VenturaHR</display-name>
  <welcome-file-list>
    <welcome-file>cadastroEmpresa.html</welcome-file>
    <welcome-file>cadastroCandidato.html</welcome-file>
    <welcome-file>login.html</welcome-file>
    <welcome-file>confirmacao.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>EmpresaServlet</display-name>
    <servlet-name>EmpresaServlet</servlet-name>
    <servlet-class>ventura.model.app.EmpresaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmpresaServlet</servlet-name>
    <url-pattern>/EmpresaServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CandidatoServlet</display-name>
    <servlet-name>CandidatoServlet</servlet-name>
    <servlet-class>ventura.model.app.CandidatoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CandidatoServlet</servlet-name>
    <url-pattern>/cadastro-candidato</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>ventura.model.app.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
</web-app>